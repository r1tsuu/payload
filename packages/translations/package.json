{
  "name": "@r1tsu/translations",
  "version": "3.0.0-beta.28",
  "main": "./src/exports/index.ts",
  "types": "./src/types.ts",
  "type": "module",
  "homepage": "https://payloadcms.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/payloadcms/payload.git",
    "directory": "packages/translations"
  },
  "scripts": {
    "build:types": "tsc --outDir dist",
    "build": "pnpm build:types",
    "clean": "rimraf {dist,*.tsbuildinfo}",
    "prepublishOnly": "pnpm clean && pnpm turbo build"
  },
  "exports": {
    ".": {
      "import": "./src/exports/index.ts",
      "require": "./src/exports/index.ts"
    },
    "./*": {
      "import": "./src/exports/*.ts",
      "require": "./src/exports/*.ts"
    },
    "./languages/*": {
      "import": "./src/languages/*.ts",
      "require": "./src/languages/*.ts"
    }
  },
  "publishConfig": {
    "access": "public",
    "main": "./dist/exports/index.js",
    "types": "./dist/exports/index.d.ts",
    "exports": {
      ".": {
        "import": "./dist/exports/index.js",
        "require": "./dist/exports/index.js",
        "types": "./dist/exports/*.d.ts"
      },
      "./*": {
        "import": "./dist/exports/*.js",
        "require": "./dist/exports/*.js",
        "types": "./dist/exports/*.d.ts"
      },
      "./languages/*": {
        "import": "./dist/languages/*.js",
        "require": "./dist/languages/*.js",
        "types": "./dist/languages/*.d.ts"
      }
    }
  },
  "devDependencies": {
    "@payloadcms/eslint-config": "workspace:*",
    "@swc/core": "^1.3.102",
    "@types/react": "18.2.74",
    "date-fns": "3.3.1",
    "typescript": "5.4.4"
  },
  "files": [
    "dist"
  ]
}
